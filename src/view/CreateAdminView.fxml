<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateAdminController">
   <children>
      <Pane layoutX="26.0" layoutY="41.0" prefHeight="467.0" prefWidth="554.0">
         <children>
            <Label alignment="CENTER" layoutX="138.0" layoutY="6.0" prefHeight="29.0" prefWidth="254.0" text="Create Account">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <VBox layoutX="77.0" layoutY="46.0" prefHeight="390.0" prefWidth="384.0">
               <children>
                  <HBox prefHeight="47.0" prefWidth="353.0" spacing="10.0">
                     <children>
                        <TextField fx:id="firstNameField" prefHeight="25.0" prefWidth="152.0" promptText="First name">
                           <HBox.margin>
                              <Insets right="20.0" top="10.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="lastNameField" prefHeight="25.0" prefWidth="130.0" promptText="Last name">
                           <HBox.margin>
                              <Insets top="10.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox prefHeight="26.0" prefWidth="384.0" spacing="35.0">
                     <children>
                        <TextField fx:id="plateNumberF" prefHeight="25.0" prefWidth="156.0" promptText="Plate Number" />
                        <ComboBox fx:id="stateBox" prefHeight="25.0" prefWidth="73.0" promptText="State">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </ComboBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="30.0" prefWidth="384.0" spacing="10.0">
                     <children>
                        <TextField fx:id="usernameField" prefHeight="29.0" prefWidth="226.0" promptText="username" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" right="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <Label fx:id="userNameLabel" prefHeight="14.0" prefWidth="228.0" textFill="#e40f0f" />
                  <HBox prefHeight="21.0" prefWidth="384.0">
                     <children>
                        <TextField fx:id="passwordField" prefHeight="29.0" prefWidth="226.0" promptText="password">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox prefHeight="38.0" prefWidth="384.0">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <children>
                        <Button mnemonicParsing="false" onAction="#createAdminAccount" text="Submit">
                           <HBox.margin>
                              <Insets left="100.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </Pane>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToHomePage" text="Home" />
   </children>
</AnchorPane>
